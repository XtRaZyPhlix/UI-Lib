local SimpleGui = Tabs.Misc:AddKeybind("Toggle SimpleSpy2 Gui", {
    Title = "Toggle SimpleSpy2 Gui",
    Mode = "Toggle",
    Default = "Z",
    Callback = function()
        local spyGui = game.CoreGui:FindFirstChild("SimpleSpy2")
        if spyGui then
            spyGui.Enabled = not spyGui.Enabled
        end
    end
})



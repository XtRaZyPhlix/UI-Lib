-- Reach Adjustment (e.g., Sword Reach)
local XValue = Tabs.Reach:AddSlider("X Value", { Title = "X Value", Min = 1, Max = 2048, Default = 1, Callback = function(Value) X = Value end })
local YValue = Tabs.Reach:AddSlider("Y Value", { Title = "Y Value", Min = 1, Max = 2048, Default = 1, Callback = function(Value) Y = Value end })
local ZValue = Tabs.Reach:AddSlider("Z Value", { Title = "Z Value", Min = 1, Max = 2048, Default = 1, Callback = function(Value) Z = Value end })

Tabs.Reach:AddButton({
    Title = "Apply Sword Reach ‚öîÔ∏è",
    Callback = function()
        local plr = game:GetService("Players").LocalPlayer
        for _, tool in pairs(plr.Character:GetDescendants()) do
            if tool:IsA("Tool") then
                tool.Handle.Size = Vector3.new(X, Y, Z)
                tool.Handle.Massless = true
                tool.Handle.CanCollide = false
                tool.Handle.Transparency = 1
                tool.GripPos = Vector3.new(0, 0, 0)
                plr.Character.Humanoid:UnequipTools()
            end
        end
    end
})

Tabs.Gamepass:AddParagraph({
    Title = "To Use The Gamepasses You Need to execute while Loading Data",
    Content = "and Click The Gamepasses You Want"
})

